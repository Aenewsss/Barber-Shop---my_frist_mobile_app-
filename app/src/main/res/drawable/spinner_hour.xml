<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/alm_transparent" />
            <size android:width="200dp" android:height="50dp" />
            <corners android:topLeftRadius="10dp" android:topRightRadius="10dp" />
        </shape>
    </item>
    <item android:gravity="bottom">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <size android:width="200dp" android:height="5dp" />
            <corners android:bottomLeftRadius="30dp" android:bottomRightRadius="30dp"/>
        </shape>
    </item>
    <item android:gravity="end" android:right="7dp" android:bottom="13dp">
        <shape android:shape="line">
            <size android:width="27dp"/>
            <stroke android:width="1dp" android:color="@color/white" />
        </shape>
    </item>
    <item android:gravity="end" android:right="17dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="line">
                <size android:width="20dp"/>
                <stroke android:width="1dp" android:color="@color/white" />
            </shape>
        </rotate>
    </item>
    <item android:gravity="end" android:right="4dp">
        <rotate android:fromDegrees="135">
            <shape android:shape="line">
                <size android:width="20dp"/>
                <stroke android:width="1dp" android:color="@color/white" />
            </shape>
        </rotate>
    </item>
</layer-list>